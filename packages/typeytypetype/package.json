{
    "name": "@captainpants/typeytypetype",
    "version": "0.0.1",
    "repository": "https://github.com/CaptainPants/typeytypetype",
    "author": "Stuart Murray <captainpantsnpm@gmail.com>",
    "license": "MIT",
    "private": false,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./umd.js": "./dist/umd.js",
        "./umd.min.js": "./dist/umd.min.js"
    },
    "scripts": {
        "format": "npx prettier --config ../../.prettierrc.json --write src",
        "lint": "npx eslint src/**/*.{ts,tsx} --fix",
        "compile": "tsc",
        "test": "npx jest --silent=false src",
        "test:only": "npx jest --silent=false",
        "build": "yarn format && yarn lint && yarn test && yarn compile"
    },
    "devDependencies": {},
    "dependencies": {
        "fast-equals": "^4.0.3"
    }
}
